{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "user_type": {
      "type": "enumeration",
      "enum": [
        "Site User",
        "Backend User"
      ]
    },
    "phone": {
      "type": "string"
    },
    "phone_alt": {
      "type": "string"
    },
    "real_estates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::real-estate.real-estate",
      "mappedBy": "user"
    },
    "contact_lists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contact-list.contact-list",
      "mappedBy": "user"
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "business_listings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::business-listing.business-listing",
      "mappedBy": "author"
    },
    "advertisements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::advertisement.advertisement",
      "mappedBy": "author"
    }
  }
}
