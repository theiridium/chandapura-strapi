{
  "kind": "collectionType",
  "collectionName": "job_listings",
  "info": {
    "singularName": "job-listing",
    "pluralName": "job-listings",
    "displayName": "Job Listing",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "publish_status": {
      "type": "boolean",
      "default": false
    },
    "step_number": {
      "type": "integer"
    },
    "slug": {
      "type": "text"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "job_listings"
    },
    "area": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::area.area"
    },
    "logo_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "component": "contact.contact-details"
    },
    "payment_details": {
      "type": "component",
      "repeatable": false,
      "component": "payment.payment"
    },
    "payment_history": {
      "type": "component",
      "repeatable": true,
      "component": "payment.payment"
    },
    "preferred_languages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::language.language"
    },
    "details_by_jobCategory": {
      "type": "dynamiczone",
      "components": [
        "job.personal-job-details",
        "job.corporate-job-details"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Corporate",
        "Personal"
      ]
    },
    "job_title": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male only",
        "Female only",
        "Any gender"
      ]
    },
    "full_address": {
      "type": "text"
    },
    "job_description": {
      "type": "richtext"
    },
    "isHired": {
      "type": "boolean",
      "default": false
    }
  }
}
