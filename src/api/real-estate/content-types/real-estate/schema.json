{
  "kind": "collectionType",
  "collectionName": "real_estates",
  "info": {
    "singularName": "real-estate",
    "pluralName": "real-estates",
    "displayName": "Real Estate",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "publish_status": {
      "type": "boolean",
      "default": false
    },
    "property_details": {
      "type": "component",
      "repeatable": false,
      "component": "real-estate.real-estate"
    },
    "amenities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::real-estate-amenity.real-estate-amenity"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "real_estates"
    },
    "description": {
      "type": "text"
    },
    "step_number": {
      "type": "integer"
    },
    "location": {
      "type": "customField",
      "customField": "plugin::google-maps.location-picker"
    },
    "payment_details": {
      "type": "component",
      "repeatable": false,
      "component": "payment.payment"
    },
    "payment_history": {
      "type": "component",
      "repeatable": true,
      "component": "payment.payment"
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "component": "contact.contact-details"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "gallery_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "property_type": {
      "type": "enumeration",
      "enum": [
        "Apartment",
        "Individual",
        "Villa"
      ]
    },
    "listing_type": {
      "type": "enumeration",
      "enum": [
        "Rent",
        "Sale"
      ]
    },
    "room_type": {
      "type": "string"
    },
    "area": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::area.area"
    },
    "full_address": {
      "type": "text"
    }
  }
}
